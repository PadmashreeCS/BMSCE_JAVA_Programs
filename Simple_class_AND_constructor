// Create a Rectangle class with fields for length and width. Include a constructor to initialize these fields and a method calculateArea() to return the area.

import java.util.*;
class Rectangle
{
    int length,width;
    Rectangle(int l, int w)
    {
        length=l;
        width=w;
    }
    int calculateArea()
    {
        return length*width;
    }
}
public class Main
{
	public static void main(String[] args)
	{
	   Rectangle R = new Rectangle(12,10);
	   int area = R.calculateArea();
	   System.out.println("Area: "+area);
	}
}
